{
    "job_id": "JOB_ID",
    "query_delay": "60s",
    "frequency": "450s",
    "indexes": [
      "filebeat-*"
    ],
    "query": {
      "bool": {
        "filter": [
          { "term":  { "fileset.module": "nginx-request-body" } },
          { "term":  { "fileset.name": "requestbody" } }
        ]
      }
    }
}
