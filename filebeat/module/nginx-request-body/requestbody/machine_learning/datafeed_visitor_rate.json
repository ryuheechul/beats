{
    "job_id": "JOB_ID",
    "query_delay": "60s",
    "frequency": "450s",
    "indexes": [
      "filebeat-*"
    ],
    "query": {
      "bool": {
        "filter": [
          { "term":  { "fileset.module": "nginx-request-body" } },
          { "term":  { "fileset.name": "requestbody" } }
        ]
      }
    },
    "aggregations": {
      "buckets": {
        "date_histogram": {
          "field": "@timestamp",
          "interval": 900000,
          "offset": 0,
          "order": {
            "_key": "asc"
          },
          "keyed": false,
          "min_doc_count": 0
        },
        "aggregations": {
          "@timestamp": {
            "max": {
              "field": "@timestamp"
            }
          },
          "dc_remote_ips": {
            "cardinality": {
              "field": "nginx-request-body.requestbody.remote_ip"
            }
          }
        }
      }
    }
}
